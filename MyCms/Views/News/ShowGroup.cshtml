@model IEnumerable<DataLayer.ShowGroupViewModel>

<div class="col-lg-4">
    <div class="mn-list">
        <h2>Categories</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li><a href=""> @item.GroupTitle <span>@item.PageCount</span></a></li>
            }

        </ul>
    </div>
</div>
